    "#jeito razo√°vel: Modelo Recursivo\n",
    "#por recorrencia : psi_n = psi_n-2 + psi_n-1 , com psi_2 = 1 e a =b = 1\n",
    "\n",
    "def fibonacci(i):\n",
    "    if i==1:\n",
    "        return 1\n",
    "    if i==0:\n",
    "        return 0\n",
    "    else:\n",
    "        return fibonacci(i-2) + fibonacci(i-1)\n",
    "\n",
    "fibonacci(1)\n",
    "#1,1,2,3,5,8,13,21,35,55\n"
   ]
  }
